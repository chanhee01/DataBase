<h1>{{main_title}}</h1>

<!--Building-->
<h2>{{title1}}</h2>

<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
    </tr>
    {{#each Building}}
    <tr>
        <td>{{"building_id"}}</td>
        <td>{{Name}}</td>
    </tr>
    {{/each}}
</table>

<!--Department-->
<h2>{{title2}}</h2>

<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Email</td>
        <td>Phone number</td>
    </tr>
    {{#each Department}}
    <tr>
        <td>{{"department_id"}}</td>
        <td>{{Name}}</td>
        <td>{{Email}}</td>
        <td>{{"Phone_number"}}</td>
    </tr>
    {{/each}}
</table>

<!--Room-->
<h2>{{title3}}</h2>

<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Capacity</td>
        <td>Building Id</td>
        <td>Department Id</td>
    </tr>
    {{#each Room}}
    <tr>
        <td>{{"room_id"}}</td>
        <td>{{Name}}</td>
        <td>{{Capacity}}</td>
        <td>{{building_id}}</td>
        <td>{{department_id}}</td>
    </tr>
    {{/each}}
</table>

